<tr id="<%= dom_id(prompt) %>" class="bg-white border-b dark:bg-gray-800 dark:border-gray-700 hover:bg-gray-50 dark:hover:bg-gray-600">
  <td class="text-gray-900 whitespace-nowrap dark:text-white">
    <div class="text-base"><%= prompt.name %></div>
  </td>
  <td class="py-4">
    <%= prompt.code %>
  </td>
  <td class="px-4 py-4 text-center space-x-2">
    <%= link_to 'Edit/View', edit_prompt_path(prompt), class: "inline-block" %>
    <%#= button_to 'Delete', prompt_path(prompt), method: :delete, data: { controller: 'confirmation', confirmation_message_value: 'Are you sure?', action: 'click->confirmation#confirm' }, class: "" %>
  </td>
</tr>